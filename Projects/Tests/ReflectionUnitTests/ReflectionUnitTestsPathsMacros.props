<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <ReflectionUnitTestsName>ReflectionUnitTests</ReflectionUnitTestsName>
    <ReflectionUnitTestsLibDir>$(EngineLibDir)$(ReflectionUnitTestsName)\</ReflectionUnitTestsLibDir>
    <ReflectionUnitTestsSourceDir>$(EngineSourceDir)\Tests\$(ReflectionUnitTestsName)\</ReflectionUnitTestsSourceDir>
    <ReflectionUnitTestsFileName>$(ReflectionUnitTestsName)_$(Configuration)_$(PlatformShortName)_$(PlatformToolset)</ReflectionUnitTestsFileName>
    <ReflectionUnitTestsBuildDir>$(EngineBuildDir)$(ReflectionUnitTestsName)_$(Configuration)_$(PlatformShortName)_$(PlatformToolset)\</ReflectionUnitTestsBuildDir>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="ReflectionUnitTestsName">
      <Value>$(ReflectionUnitTestsName)</Value>
    </BuildMacro>
    <BuildMacro Include="ReflectionUnitTestsLibDir">
      <Value>$(ReflectionUnitTestsLibDir)</Value>
    </BuildMacro>
    <BuildMacro Include="ReflectionUnitTestsSourceDir">
      <Value>$(ReflectionUnitTestsSourceDir)</Value>
    </BuildMacro>
    <BuildMacro Include="ReflectionUnitTestsFileName">
      <Value>$(ReflectionUnitTestsFileName)</Value>
    </BuildMacro>
    <BuildMacro Include="ReflectionUnitTestsBuildDir">
      <Value>$(ReflectionUnitTestsBuildDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>