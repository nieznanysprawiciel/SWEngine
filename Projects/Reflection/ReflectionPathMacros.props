<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <ReflectionName>Reflection</ReflectionName>
    <ReflectionLibDir>$(EngineLibDir)$(ReflectionName)\$(PlatformToolset)\$(PlatformShortName)\$(ConfigTypeString)\</ReflectionLibDir>
    <ReflectionSourceDir>$(EngineSourceDir)Tools\$(ReflectionName)\</ReflectionSourceDir>
    <ReflectionFileName>$(ReflectionName)</ReflectionFileName>
    <ReflectionBuildDir>$(EngineBuildDir)$(ReflectionName)_$(Configuration)_$(PlatformShortName)_$(PlatformToolset)\</ReflectionBuildDir>
  </PropertyGroup>

  <ItemGroup>
    <BuildMacro Include="ReflectionName">
      <Value>$(ReflectionName)</Value>
    </BuildMacro>
    <BuildMacro Include="ReflectionLibDir">
      <Value>$(ReflectionLibDir)</Value>
    </BuildMacro>
    <BuildMacro Include="ReflectionSourceDir">
      <Value>$(ReflectionSourceDir)</Value>
    </BuildMacro>
    <BuildMacro Include="ReflectionFileName">
      <Value>$(ReflectionFileName)</Value>
    </BuildMacro>
    <BuildMacro Include="ReflectionBuildDir">
      <Value>$(ReflectionBuildDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>