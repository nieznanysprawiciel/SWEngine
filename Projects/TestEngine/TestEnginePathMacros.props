<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <TestEngineName>TestEngine</TestEngineName>
    <TestEngineLibDir>$(EngineLibDir)$(TestEngineName)\</TestEngineLibDir>
    <TestEngineSourceDir>$(EngineSourceDir)$(TestEngineName)\</TestEngineSourceDir>
    <TestEngineFileName>$(TestEngineName)_$(Configuration)_$(PlatformShortName)</TestEngineFileName>
    <TestEngineBuildDir>$(EngineBuildDir)$(TestEngineName)_$(Configuration)_$(PlatformShortName)\</TestEngineBuildDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="TestEngineName">
      <Value>$(TestEngineName)</Value>
    </BuildMacro>
    <BuildMacro Include="TestEngineLibDir">
      <Value>$(TestEngineLibDir)</Value>
    </BuildMacro>
    <BuildMacro Include="TestEngineSourceDir">
      <Value>$(TestEngineSourceDir)</Value>
    </BuildMacro>
    <BuildMacro Include="TestEngineFileName">
      <Value>$(TestEngineFileName)</Value>
    </BuildMacro>
    <BuildMacro Include="TestEngineBuildDir">
      <Value>$(TestEngineBuildDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>